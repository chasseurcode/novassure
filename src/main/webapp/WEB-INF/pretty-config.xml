<?xml version="1.0" encoding="UTF-8"?>
<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">

	<url-mapping id="login">
		<pattern value="/login" />
		<view-id value="/login.xhtml" />
	</url-mapping>

	<url-mapping id="home">
		<pattern value="/" />
		<view-id value="/index.xhtml" />
	</url-mapping>

	<!-- Users -->
	<url-mapping id="utilisateurs">
		<pattern value="/utilisateurs" />
		<view-id value="/pages/users/index.xhtml" />
	</url-mapping>

	<!-- Clients -->
	<url-mapping id="clients">
		<pattern value="/clients/create" />
		<view-id value="/pages/clients/create.xhtml" />
	</url-mapping>
	<url-mapping id="ficheclient">
		<pattern value="/clients/#{cliendId}/fiche" />
		<view-id value="/pages/clients/fiche.xhtml" />
	</url-mapping>

	<!-- Affaires -->
	<url-mapping id="affaires">
		<pattern value="/affaires" />
		<view-id value="/pages/affaires/index.xhtml" />
	</url-mapping>


	<!-- Agents -->
	<url-mapping id="agents">
		<pattern value="/agents" />
		<view-id value="/pages/agents/index.xhtml" />
	</url-mapping>

	<!-- Compagnies -->
	<url-mapping id="compagnies">
		<pattern value="/compagnies" />
		<view-id value="/pages/compagnies/index.xhtml" />
	</url-mapping>

	<!-- Branches -->
	<url-mapping id="branches">
		<pattern value="/branches" />
		<view-id value="/pages/branches/index.xhtml" />
	</url-mapping>

	<!--Quittances -->
	<url-mapping id="quittances">
		<pattern value="/quittances/#{quittanceBean.numPolice}" />
		<view-id value="/pages/quittances/detail.xhtml" />
	</url-mapping>

	<!-- Type de Quittances -->
	<url-mapping id="tquittances">
		<pattern value="/types/quittances" />
		<view-id value="/pages/typequittances/index.xhtml" />
	</url-mapping>

</pretty-config>